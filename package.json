{
  "name": "vegasquad",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "build:css": "postcss src/css/App.css -o src/index.css",
    "watch:css": "postcss src/css/App.css -o src/index.css -w",
    "start": "yarn watch:css & react-scripts start",
    "build": "yarn build:css && react-scripts build",
    "now-build": "yarn build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "postcss-cli": "^6.1.2",
    "tailwindcss": "^0.7.4"
  }
}
